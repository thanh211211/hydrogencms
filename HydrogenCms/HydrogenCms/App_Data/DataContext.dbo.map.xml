<?xml version="1.0" encoding="utf-8"?>
<Database Name="HydrogenCms" xmlns="http://schemas.microsoft.com/linqtosql/mapping/2007">
	<Table Name="dbo.hcms_Setting" Member="Settings">
		<Type Name="HydrogenCms.Models.Setting">
